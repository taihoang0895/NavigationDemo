<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Navigation Graph Fragment 1" />
        <Button
            android:id="@+id/start_with_pop_up_to_inclusize_false_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="start width popUpToInclusive=false"/>
        <Button
            android:id="@+id/start_with_pop_up_to_inclusize_true_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="start width popUpToInclusive=true"/>
    </LinearLayout>
</layout>